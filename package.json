    "@aws-cdk/aws-cloudfront-origins": "1.60.0",
    "@aws-cdk/aws-ec2": "1.60.0",
    "@aws-cdk/aws-ecr": "1.60.0",
    "@aws-cdk/aws-ecs-patterns": "1.60.0",
    "@aws-cdk/aws-iam": "1.60.0",
    "@aws-cdk/aws-rds": "1.60.0",
    "@aws-cdk/core": "1.60.0",
    "@types/jest": "^25.2.1",
    "@types/js-yaml": "^3.12.5",
    "@types/md5": "^2.2.0",
