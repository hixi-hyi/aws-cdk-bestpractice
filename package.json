{
  "name": "aws-cdk-bestpractice",
  "version": "0.1.0",
  "bin": {
    "aws-cdk-bestpractice": "bin/aws-cdk-bestpractice.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "./mycdk"
