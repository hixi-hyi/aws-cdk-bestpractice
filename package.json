    "jsdom": "^16.4.0",
    "md5": "^2.3.0",
    "ncu": "^0.2.1",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "~3.7.2",
    "utf-8-validate": "^5.0.2",
    "ws": "^7.3.1",
    "yesno": "^0.3.1"
